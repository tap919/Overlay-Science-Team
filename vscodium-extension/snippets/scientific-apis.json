{
  "PubChemPy Compound Search": {
    "prefix": "pubchem-search",
    "body": [
      "import pubchempy as pcp",
      "",
      "# Search for compound by name",
      "compounds = pcp.get_compounds('${1:aspirin}', 'name')",
      "for compound in compounds:",
      "    print(f'Name: {compound.iupac_name}')",
      "    print(f'Formula: {compound.molecular_formula}')",
      "    print(f'MW: {compound.molecular_weight}')",
      "    print(f'SMILES: {compound.isomeric_smiles}')",
      "$0"
    ],
    "description": "Search PubChem for compound information"
  },
  "Biopython PubMed Query": {
    "prefix": "biopython-pubmed",
    "body": [
      "from Bio import Entrez",
      "",
      "# Set your email (required by NCBI)",
      "Entrez.email = '${1:your@email.com}'",
      "",
      "# Search PubMed",
      "handle = Entrez.esearch(db='pubmed', term='${2:cancer research}', retmax=${3:10})",
      "record = Entrez.read(handle)",
      "handle.close()",
      "",
      "# Fetch article details",
      "for pmid in record['IdList']:",
      "    fetch_handle = Entrez.efetch(db='pubmed', id=pmid, retmode='xml')",
      "    article = Entrez.read(fetch_handle)",
      "    fetch_handle.close()",
      "    print(article)",
      "$0"
    ],
    "description": "Query PubMed using Biopython Entrez"
  },
  "BioThings Gene Query": {
    "prefix": "biothings-gene",
    "body": [
      "from biothings_client import get_client",
      "",
      "# Get gene client",
      "mg = get_client('gene')",
      "",
      "# Query multiple genes",
      "genes = ['${1:TP53}', '${2:BRCA1}', '${3:EGFR}']",
      "results = mg.querymany(genes, scopes='symbol', fields='symbol,name,summary,pathway')",
      "",
      "for result in results:",
      "    print(f\"Gene: {result.get('symbol', 'N/A')}\")",
      "    print(f\"Name: {result.get('name', 'N/A')}\")",
      "    print(f\"Summary: {result.get('summary', 'N/A')[:200]}...\")",
      "    print('---')",
      "$0"
    ],
    "description": "Query gene information using BioThings"
  },
  "Science Pipeline Setup": {
    "prefix": "science-pipeline-setup",
    "body": [
      "#!/usr/bin/env python3",
      "\"\"\"",
      "Science Pipeline Study",
      "\"\"\"",
      "",
      "from pathlib import Path",
      "import json",
      "",
      "# Study configuration",
      "STUDY_ID = '${1:study_001}'",
      "STUDY_NAME = '${2:Protein Folding Analysis}'",
      "",
      "# Setup directories",
      "workspace = Path('science_workspace')",
      "input_dir = workspace / 'input' / STUDY_ID",
      "output_dir = workspace / 'deliverables' / STUDY_ID",
      "",
      "input_dir.mkdir(parents=True, exist_ok=True)",
      "output_dir.mkdir(parents=True, exist_ok=True)",
      "",
      "# Study metadata",
      "metadata = {",
      "    'study_id': STUDY_ID,",
      "    'study_name': STUDY_NAME,",
      "    'timestamp': '${3:2026-02-04}',",
      "    'sources': [],",
      "    'parameters': {",
      "        'chaos_coefficient': ${4:0.7},",
      "        'iterations': ${5:10000}",
      "    }",
      "}",
      "",
      "# Save metadata",
      "with open(input_dir / 'metadata.json', 'w') as f:",
      "    json.dump(metadata, f, indent=2)",
      "",
      "print(f'Study {STUDY_ID} initialized')",
      "print(f'Input directory: {input_dir}')",
      "print(f'Output directory: {output_dir}')",
      "$0"
    ],
    "description": "Setup a new science pipeline study"
  },
