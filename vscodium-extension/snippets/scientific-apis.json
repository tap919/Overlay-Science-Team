  "Data Analysis Pipeline": {
    "prefix": "data-analysis-pipeline",
    "body": [
      "import pandas as pd",
      "import numpy as np",
      "import matplotlib.pyplot as plt",
      "from scipy import stats",
      "",
      "# Load data",
      "data = pd.read_csv('${1:data.csv}')",
      "",
      "# Basic statistics",
      "print(data.describe())",
      "",
      "# Correlation analysis",
      "correlation_matrix = data.corr()",
      "print(correlation_matrix)",
      "",
      "# Statistical testing",
      "group1 = data[data['${2:group}'] == '${3:A}']['${4:value}']",
      "group2 = data[data['${2:group}'] == '${5:B}']['${4:value}']",
      "t_stat, p_value = stats.ttest_ind(group1, group2)",
      "print(f'T-statistic: {t_stat:.4f}, P-value: {p_value:.4f}')",
      "",
      "# Visualization",
      "plt.figure(figsize=(10, 6))",
      "data.boxplot(column='${4:value}', by='${2:group}')",
      "plt.title('${6:Comparison Plot}')",
      "plt.savefig('${7:output.png}')",
      "$0"
    ],
    "description": "Complete data analysis pipeline"
  }
